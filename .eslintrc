{
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "rules": {
    "consistent-return": "off",
    "eqeqeq": "off",
    "func-names": "off",
    "global-require": "off",
    "import/no-extraneous-dependencies": "off",
    "indent": "off",
    "no-console": "off",
    "no-empty": "off",
    "no-loop-func": "off",
    "no-plusplus": "off",
    "no-else-return": "off",
    "no-return-assign": "off",
    "no-sequences": "off",
    "no-undef": "off",
    "no-unreachable": "off",
    "no-var": "off",
    "no-unused-expressions": "off",
    "one-var": "off",
    "prefer-const": "off",
    "prefer-destructuring": "off",
    "semi": "off",
    "space-before-function-paren": "off",
    "strict": "off",
    "wrap-iife": "off"
  }
}
